
/* Test casts from tiny-int */
{
  assert((tiny)  42T == 42T);
  assert((utiny) 42T == 42UT);
  assert((short) 42T == 42S);
  assert((ushort)42T == 42US);
  assert((int)   42T == 42I);
  assert((uint)  42T == 42UI);
  assert((long)  42T == 42L);
  assert((ulong) 42T == 42UL);
  assert((float) 42T == 42F);
  assert((double)42T == 42D);
  assert((char)  42T == '*');
}

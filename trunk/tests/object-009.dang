// PURPOSE: test objects containing tensors

object X
{
  new () { }
  public matrix<string> y;
  public vector<int> z;
}

{
  X x = new X();
  x.y = [["hi" "mom"] ["what" "ever"]];
  x.z = [42 32 12 2];
  assert(x.y == [["hi" "mom"] ["what" "ever"]]);
  assert(x.z == [42 32 12 2]);
  x.y[0,1] = "foo";
  assert(x.y == [["hi" "foo"] ["what" "ever"]]);
  x.z[3] = 92;
  assert(x.z == [42 32 12 92]);
}

// PURPOSE: test concat([[]])
assert(concat([[1 2 3] ([1 3 5]) [3 ] [1 2 3 4]]) == [1 2 3 1 3 5 3 1 2 3 4]);
assert(concat([   ([[1 2] [2 3]])
                  ([[3 4] [4 5]])  ]) == [[1 2]
		                          [2 3]
					  [3 4]
					  [4 5]]);

assert(concat([["a" "b"] ["c" "d" "e"]]) == ["a" "b" "c" "d" "e"]);
